{
    "name": "avrgirl-arduino-electron",
    "version": "0.0.1",
    "description": "Electron wrapper of AVRGIRL Arduino programmer",
    "author": "Ruben Dax",
    "license": "MIT",
    "homepage": "https://rubendax.com",
    "keywords": [
        "arduino",
        "avr",
        "avrgirl",
        "firmware",
        "microcontroller"
    ],
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "npx electron-builder"
    },
    "build": {
        "appId": "com.rubendax.app",
        "asarUnpack": [
            "**/*.node"
        ],
        "directories": {
            "buildResources": "build",
            "output": "dist"
        }
    },
    "dependencies": {
        "avrgirl-arduino": "^4.3.1",
        "fs": "0.0.1-security",
        "open": "^7.3.0",
        "path": "^0.12.7"
    },
    "devDependencies": {
        "electron": "^7.3.3",
        "electron-builder": "^22.9.1",
        "electron-rebuild": "^2.3.2"
    }
}
